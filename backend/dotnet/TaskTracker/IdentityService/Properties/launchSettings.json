{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "http://localhost:5043",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "LoggerOptions__UseLogger": "serilog",
        "Serilog__MinimumLevel__Default": "Information"
      }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
//      "launchUrl": "swagger",
      "launchUrl": ".well-known/openid-configuration",
      "applicationUrl": "https://localhost:7003;http://localhost:5043",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "LoggerOptions__UseLogger": "serilog",
        "Serilog__MinimumLevel__Default": "Information"
      }
    }
  }
}
